<div class="col-sm-6 column-block about-ltlbr">
  <article class="col-sm-12 grid-tile">
    {% if row.col_1_r1_1_type == "text" %}
      {% include 'blocks/text.twig' with {
        title : row.col_1_r1_1_title,
        text : row.col_1_r1_1_text,
        image : theme.link ~ "/assets/aboutpage/about-black.png",
        theme : theme
      } only %}
    {% else %}
      {% include 'blocks/image.twig' with {
        image : TimberImage(row.col_1_r1_1_image),
        theme : theme
      } only %}
    {% endif %}
  </article>
